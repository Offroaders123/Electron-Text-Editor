{
  "name": "electron-text-editor",
  "version": "0.2.0",
  "author": "Brandon Bennett",
  "homepage": "https://github.com/Offroaders123/Electron-Text-Editor",
  "description": "Electron Text Editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.offroaders123.electronTextEditor",
    "productName": "Electron Text Editor",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1"
  }
}