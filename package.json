{
  "name": "electron-text-editor",
  "version": "0.4.0",
  "description": "A native text editor built using Electron!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Offroaders123/Electron-Text-Editor.git"
  },
  "author": "Brandon Bennett <offroaders123@gmail.com> (https://github.com/Offroaders123)",
  "license": "MIT",
  "homepage": "https://github.com/Offroaders123/Electron-Text-Editor#readme",
  "build": {
    "appId": "com.offroaders123.electronTextEditor",
    "productName": "Electron Text Editor",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  },
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13"
  }
}