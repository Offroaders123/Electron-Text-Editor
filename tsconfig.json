{
  "compilerOptions": {
    "outDir": "./dist-ts",
    "module": "NodeNext",
    "target": "ESNext",
    "isolatedModules": true,
    "types": [
      "@stedit/num-text",
      "electron-nightly"
    ],
    // "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}